class Solution {
    public int hIndex(int[] citations) {
        Arrays.sort(citations);
        int h=0;
        for(int i=0;i<citations.length;i++){
            int paperleast=citations.length-i;
            if(citations[i]>=paperleast){
                h=paperleast;
                break;
            }
        }
        return h;
    }
}
